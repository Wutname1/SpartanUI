<Ui xmlns="http://www.blizzard.com/wow/ui/" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
	<!-- frameStrata Parameters:  BACKGROUND, LOW, MEDIUM, HIGH, DIALOG, FULLSCREEN, FULLSCREEN_DIALOG, TOOLTIP  -->
	<!-- Layer level Parameters:  BACKGROUND, BORDER, ARTWORK, OVERLAY, HIGHLIGHT -->
	<Script file="Style-Setup.lua" />

	<!-- Transparent_AnchorFrame -->
	<Frame name="Transparent_AnchorFrame" frameStrata="BACKGROUND" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT">
				<Offset>
					<AbsDimension x="0" y="-1"/>
				</Offset>
			</Anchor>
			<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT">
				<Offset>
					<AbsDimension x="0" y="-1"/>
				</Offset>
			</Anchor>
		</Anchors>
	</Frame>

	<!-- SpartanUI -->
	<Frame name="Transparent_SpartanUI" frameStrata="BACKGROUND" parent="UIParent">
		<Anchors>
			<Anchor point="BOTTOMLEFT" relativeTo="Transparent_AnchorFrame" relativePoint="TOPLEFT" />
			<Anchor point="TOPRIGHT" relativeTo="Transparent_AnchorFrame" relativePoint="TOPRIGHT">
				<Offset>
					<AbsDimension x="0" y="153"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parent_Base1" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\base-center">
					<Anchors>
						<Anchor point="BOTTOM" relativePoint="BOTTOM"/>
					</Anchors>
				</Texture>
				<Texture name="$parent_Base2" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\base-sides">
					<Anchors>
						<Anchor point="RIGHT" relativeTo="$parent_Base1" relativePoint="LEFT"/>
					</Anchors>
				</Texture>
				<Texture name="$parent_Base3" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\base-sides" tile="true">
					<Anchors>
						<Anchor point="RIGHT" relativeTo="$parent_Base2" relativePoint="LEFT"/>
					</Anchors>
				</Texture>
				<Texture name="$parent_Base4" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\base-sides">
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parent_Base1" relativePoint="RIGHT"/>
					</Anchors>
				</Texture>
				<Texture name="$parent_Base5" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\base-sides" tile="true">
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parent_Base4" relativePoint="RIGHT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

	<!-- Transparent_PopTemplate -->
	<Frame name="Transparent_PopTemplate" frameStrata="BACKGROUND" virtual="true">
		<Size>
			<AbsDimension x="400" y="32" />
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\bar-backdrop0">
					<Size>
						<AbsDimension x="512" y="64" />
					</Size>
					<Anchors>
						<Anchor point="BOTTOM" relativePoint="BOTTOM"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

	<!-- Transparent_BarTemplate1 -->
	<Frame name="Transparent_BarTemplate1" frameStrata="BACKGROUND" virtual="true">
		<Size>
			<AbsDimension x="400" y="32" />
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\bar-backdrop1">
					<Size>
						<AbsDimension x="512" y="64" />
					</Size>
					<Anchors>
						<Anchor point="CENTER" relativePoint="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

	<!-- Transparent_BarTemplate3 -->
	<Frame name="Transparent_BarTemplate3" frameStrata="BACKGROUND" virtual="true">
		<Size>
			<AbsDimension x="135" y="100" />
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\bar-backdrop3">
					<Size>
						<AbsDimension x="256" y="128" />
					</Size>
					<Anchors>
						<Anchor point="CENTER" relativePoint="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

	<!-- Transparent_ActionBarsTemplate -->
	<Frame name="Transparent_ActionBarsTemplate" parent="Transparent_SpartanUI" frameStrata="BACKGROUND" virtual="true">
		<Size>
			<AbsDimension x="1002" y="139"/>
		</Size>
		<Frames>
			<Frame name="Transparent_Bar1" inherits="Transparent_BarTemplate1">
				<Anchors>
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="-100" y="50" />
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="Transparent_Bar2" inherits="Transparent_BarTemplate1">
				<Anchors>
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="-100" y="5" />
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="Transparent_Bar3" inherits="Transparent_BarTemplate1">
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="100" y="50" />
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="Transparent_Bar4" inherits="Transparent_BarTemplate1">
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="100" y="5" />
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="Transparent_Bar5" inherits="Transparent_BarTemplate3">
				<Anchors>
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="-502" y="0" />
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="Transparent_Bar6" inherits="Transparent_BarTemplate3">
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="502" y="0" />
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="Transparent_Popup1" inherits="Transparent_PopTemplate">
				<Anchors>
					<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="-100" y="95" />
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
			<Frame name="Transparent_Popup2" inherits="Transparent_PopTemplate">
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="BOTTOM">
						<Offset>
							<AbsDimension x="100" y="95" />
						</Offset>
					</Anchor>
				</Anchors>
			</Frame>
		</Frames>
	</Frame>

	<!-- Transparent_Popup1MaskTemplate -->
	<Frame name="Transparent_Popup1MaskTemplate" parent="Transparent_SpartanUI" frameStrata="MEDIUM" virtual="true">
		<Size>
			<AbsDimension x="400" y="34"/>
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\bar-popup1">
					<Size>
						<AbsDimension x="512" y="64" />
					</Size>
					<Anchors>
						<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

	<!-- Transparent_Popup2MaskTemplate -->
	<Frame name="Transparent_Popup2MaskTemplate" parent="Transparent_SpartanUI" frameStrata="MEDIUM" virtual="true">
		<Size>
			<AbsDimension x="400" y="34"/>
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBG" file="Interface\AddOns\SpartanUI\\Themes\\Transparent\\\Images\bar-popup2">
					<Size>
						<AbsDimension x="512" y="64" />
					</Size>
					<Anchors>
						<Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

	<Script file="Global-Framework.lua" />
	<Script file="Module-ActionBars.lua" />
	<Script file="Module-Minimap.lua" />
	<Script file="Style.lua" />
</Ui>
